{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
        "name": "Titanic",
        "type": "debugpy",
        "request": "launch",
        "module": "automl.automl_main",
        "console": "integratedTerminal",
        "args": [
            "--config", "personal/Titanic/data/config.yaml",
        ],
        "env": {
            "UV_PREFER_BINARY": "1"
        }
        },
        {
        "name": "HousePrices",
        "type": "debugpy",
        "request": "launch",
        "module": "automl.automl_main",
        "console": "integratedTerminal",
        "args": [
            "--config", "personal/HousePrices/data/config.yaml",
        ],
        "env": {
            "UV_PREFER_BINARY": "1"
        }
        },
        {
        "name": "SpaceshipTitanic",
        "type": "debugpy",
        "request": "launch",
        "module": "automl.automl_main",
        "console": "integratedTerminal",
        "args": [
            "--config", "personal/SpaceshipTitanic/data/config.yaml",
        ],
        "env": {
            "UV_PREFER_BINARY": "1"
        }
        }
    ],
    "compounds": [
    {
        "name": "Run All Files",
        "configurations": ["Titanic", "SpaceshipTitanic", "HousePrices"]
        }
    ]
}
